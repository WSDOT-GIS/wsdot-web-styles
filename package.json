{
  "name": "@wsdot/web-styles",
  "version": "1.0.1",
  "description": "Provides WSDOT style definitions for websites",
  "type": "module",
  "main": "tools/index.js",
  "files": [
    "css/**/*.css",
    "images/**/*.{svg,png}"
  ],
  "scripts": {
    "lint": "biome lint --write",
    "format": "biome format --write",
    "biome": "biome check --write"
  },
  "keywords": [
    "wsdot",
    "color",
    "standards"
  ],
  "author": "Jeff Jacobson",
  "license": "Unlicense",
  "optionalDependencies": {
    "@fontsource/lato": "^5.2.7",
    "@fontsource/inconsolata": "^5.2.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.1",
    "@types/bun": "latest",
    "@types/jsdom": "^27.0.0",
    "jsdom": "^27.0.1",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WSDOT-GIS/wsdot-web-styles.git"
  },
  "bugs": {
    "url": "https://github.com/WSDOT-GIS/wsdot-web-styles/issues"
  },
  "homepage": "https://github.com/WSDOT-GIS/wsdot-web-styles#readme"
}