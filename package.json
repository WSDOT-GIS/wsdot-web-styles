{
  "name": "@wsdot/web-styles",
  "version": "1.0.0",
  "description": "Provides WSDOT style definitions for websites",
  "private": true,
  "type": "module",
  "main": "tools/index.js",
  "files": [
    "tools/*.js",
    "css/**/*.css",
    "images/**/*.{svg,png}"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "wsdot",
    "color",
    "standards"
  ],
  "author": "Jeff Jacobson",
  "license": "Unlicense",
  "overrides": {
    "eslint": "$eslint"
  },
  "optionalDependencies": {
    "@fontsource/lato": "^5.0.18",
    "@fontsource/inconsolata": "^5.0.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bun": "latest",
    "@types/jsdom": "^21.1.6",
    "eslint": "^9.1.1",
    "eslint-plugin-jsdoc": "^48.2.3",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WSDOT-GIS/wsdot-web-styles.git"
  },
  "bugs": {
    "url": "https://github.com/WSDOT-GIS/wsdot-web-styles/issues"
  },
  "homepage": "https://github.com/WSDOT-GIS/wsdot-web-styles#readme"
}